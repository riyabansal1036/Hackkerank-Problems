#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int a,b,c,d;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    printf("P1 (WT=0), ");
    printf("P2 (WT=%d), ",a);
    printf("P3 (WT=%d), ",a+b);
    printf("P4 (WT=%d) ",a+b+c);
    
    return 0;
}
